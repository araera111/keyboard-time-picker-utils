{
  "name": "libname",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "dev": "tsx ./src/main.ts",
    "start": "cross-env NODE_ENV=production node ./dist/main.js",
    "build": "packemon build --addEngines --addExports --declaration",
    "lint-fix": "eslint --cache --fix src/ && prettier --write src/",
    "test": "jest",
    "type": "tsc --noEmit"
  },
  "keywords": [],
  "author": "KATO Yuumin",
  "license": "MIT",
  "devDependencies": {
    "@types/eslint": "^8.44.4",
    "@types/eslint-config-prettier": "^6.11.1",
    "@types/node": "^20.8.6",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "eslint": "^8.51.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-base-typescript": "^1.1.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.4.2",
    "packemon": "^3.2.2",
    "prettier": "^3.0.3",
    "tsx": "^3.13.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "volta": {
    "node": "18.17.1",
    "npm": "9.8.1"
  },
  "packemon": {
    "platform": [
      "node",
      "browser"
    ],
    "support": "stable",
    "format": [
      "lib",
      "esm"
    ],
    "inputs": {
      "index": "src/main.ts"
    }
  },
  "main": "./lib/node/main.js",
  "types": "./lib/main.d.ts",
  "module": "./esm/index.js",
  "browser": "./lib/browser/index.js",
  "engines": {
    "node": ">=16.12.0"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./lib/main.d.ts",
      "browser": {
        "types": "./esm/main.d.ts",
        "module": "./esm/index.js",
        "import": "./esm/index.js",
        "default": "./lib/browser/index.js"
      },
      "node": {
        "types": "./lib/main.d.ts",
        "default": "./lib/node/main.js"
      },
      "default": "./lib/browser/index.js"
    }
  },
  "files": [
    "esm/**/*",
    "lib/**/*",
    "src/**/*"
  ]
}
